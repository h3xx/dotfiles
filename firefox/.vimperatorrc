" be more like Vimium
"set hintchars=sadfjklewcmpgh

" left hand keys only
"set hintchars=qazwsxedcrtfgv

" left hand, top-row keys only
set hintchars=asdfgqwert

" use bookmarks for O/o/:open completion
set complete=bsl

" faster scrolling (even arrow keys are faster)
nnoremap j 4j
nnoremap k 4k

" restore Private Mode window shortcut
nnoremap <C-S-p> :winopen -private<cr>

" vi: ft=vim
