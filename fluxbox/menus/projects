# current projects
[submenu] (projects)
	[submenu] (audicon)
		[exec] (grip) {grip} <~/.fluxbox/icons/grip.xpm>
		[exec] (scilab) {scilab -e 'cd ~/audicon'} <~/.fluxbox/icons/scilab.xpm>
		[exec] (audacity) {audacity} <~/.fluxbox/icons/audacity.xpm>
		[exec] (gentoo) {gentoo -1 ~/audicon/music -2 ~/audicon} <~/.fluxbox/icons/gentoo.xpm>
		[exec] (sqlitebrowser) {sqlitebrowser ~/audicon/samples.db}
		[exec] (easytag) {easytag ~/audicon/music} <~/.fluxbox/icons/easytag.xpm>
	[end]
[end]
